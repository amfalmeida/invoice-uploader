<service>
    <id>@projectName@</id>
    <name>@projectName@</name>
    <description>This service runs ${app.name}.</description>
    <workingdirectory>.</workingdirectory>
    <onfailure action="restart" delay="10 sec"/>
    <executable>java</executable>
    <arguments>-Xmx64M -Dlogging.config=./config/logback.xml -jar @projectName@-@projectVersion@.jar</arguments>
    <logmode>rotate</logmode>
    <logpath>./temp</logpath>
</service>